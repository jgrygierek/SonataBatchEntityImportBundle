{
    "name": "jgrygierek/sonata-batch-entity-import-bundle",
    "type": "symfony-bundle",
    "description": "Importing entities with preview and edit features for Sonata Admin.",
    "keywords": [
        "entity",
        "import",
        "preview",
        "excel",
        "csv",
        "ods",
        "symfony",
        "sonata"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaros≈Çaw Grygierek",
            "email": "jaroslawgrygierek@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "jgrygierek/batch-entity-import-bundle": "dev-master",
        "sonata-project/admin-bundle": "^3.5",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "escapestudios/symfony2-coding-standard": "3.x-dev",
        "knplabs/doctrine-behaviors": "~2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1.1",
        "nyholm/symfony-bundle-test": "^1.6",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "~3.5",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/css-selector": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^4.4|^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JG\\SonataBatchEntityImportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JG\\SonataBatchEntityImportBundle\\Tests\\": "tests/"
        }
    }
}
